@import "../common"
//页面加载样式
._load-page
    position: fixed
    top: 0px
    left: 0px
    right: 0px
    bottom: 0px
    z-index: 10000
    background-color: #f2f2f2
    overflow: hidden
    .load
        margin-left: auto
        margin-right: auto
        width: 120px
        margin-top: 50px
//列表加载更多样式
._load-more
    padding: 10px
    color: #cfcfcf
    text-align: center
    font-size: 1.2rem

